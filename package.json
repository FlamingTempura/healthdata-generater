{
  "author": "FlamingTempura",
  "name": "mhealthgen",
  "homepage": "https://github.com/FlamingTempura/mhealthgen",
  "version": "0.0.1",
  "description": "mhealthgen is a node.js library for generating realistic looking health-related information about a person in JSON format. The types of data which are generated include: blood pressure, calories burned, caloric intake, heart rate, height, weight, sleep, life satisfaction, prescriptions, clinic visits, diagnoses, and symptoms.",
  "main": "mhealthgen.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "license": "MIT",
  "dependencies": {
    "alea": "0.0.9",
    "simplex-noise": "^2.3.0",
    "uuid": "^3.1.0",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "jsdom": "^11.3.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-esformatter": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "tap": "^10.7.2"
  },
  "scripts": {
    "tap": "tap test/*",
    "build": "rollup -c",
    "watch": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlamingTempura/mhealthgen.git"
  }
}
